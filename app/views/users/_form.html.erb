<div class="row">
  <div class="col-xs-12 col-sm-4 col-sm-offset-4">
    <h1>Sign Up</h1>
    <%= simple_form_for @user do |u| %>
      <%=u.input :first_name%>
      <%=u.input :last_name%>
      <%=u.input :email%>
      <%=u.input :password%>
      <%=u.input :password_confirmation%>
      <%= u.submit %>
      <%end%>
  </div>
</div>
